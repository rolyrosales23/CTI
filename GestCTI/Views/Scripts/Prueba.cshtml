@{
    Layout = null;
}

@section css{
    <style>
        .myscript {
            background-color: darkcyan;
            padding: 0px;
            margin: 0px;
            height: 100%;
            width: 100%;
        }
    </style>
}

<div class="myscript">
    <h1>Scritp de Prueba</h1>

    @if (@ViewBag.data != null)
    {
        <h3>ANI: @ViewBag.data.callingDeviceId</h3>

        <h3>DNIS: @ViewBag.data.calledDeviceId</h3>
        <hr>
        <h2>Examples of parameters</h2>
        <h3>ucid: @ViewBag.data.ucid</h3>

        <h3>callId: @ViewBag.data.callId</h3>

        <h3>deviceId: @ViewBag.data.deviceId</h3>

        <h3>alertingDeviceId: @ViewBag.data.alertingDeviceId</h3>

        <h3>trunkGroupId: @ViewBag.data.trunkGroupId</h3>

        <h3>splitDeviceId: @ViewBag.data.splitDeviceId</h3>

        <h3>lastRedirectionDeviceId: @ViewBag.data.lastRedirectionDeviceId</h3>

        <h3>callerId: @ViewBag.data.callerId</h3>

        <h3>appName: @ViewBag.data.appName</h3>
    }

</div>